{
  "name": "minikube-action",
  "version": "0.0.3",
  "private": true,
  "description": "start a kubernetes cluster in github actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medyagh/setup-minikube"
  },
  "keywords": [
    "minikube",
    "minikube CI",
    "test aginst kubernetes",
    "test kubernetes",
    "kuberentes CI",
    "kubernetes",
    "kubernetes cluster",
    "github",
    "actions"
  ],
  "author": "medyagh",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/tool-cache": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.0.0",
    "jest-circus": "^26.0.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.4.0",
    "typescript": "^3.6.4"
  }
}
